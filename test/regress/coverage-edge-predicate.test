; Test complex predicate expressions (exercises predicate.h, expr.h)

2024/01/05 * Grocery Store
    Expenses:Food:Groceries       $45.00
    Assets:Checking

2024/01/10 * Gas Station
    Expenses:Auto:Gas             $35.00
    Assets:Checking

2024/01/15 * Restaurant
    Expenses:Food:Dining          $65.00
    Assets:Credit Card

2024/02/01 * Grocery Store
    Expenses:Food:Groceries       $52.00
    Assets:Checking

2024/02/10 * Electric Company
    Expenses:Utilities:Electric   $120.00
    Assets:Checking

test reg --limit 'account =~ /Food/ and amount >= 50' --args-only --columns=80 -> 0
24-Jan-15 Restaurant            Expenses:Food:Dining         $65.00       $65.00
24-Feb-01 Grocery Store         Expense:Food:Groceries       $52.00      $117.00
end test

test reg --limit 'payee =~ /Store/ or payee =~ /Station/' --limit 'amount > 0' --args-only --columns=80 -> 0
24-Jan-05 Grocery Store         Expense:Food:Groceries       $45.00       $45.00
24-Jan-10 Gas Station           Expenses:Auto:Gas            $35.00       $80.00
24-Feb-01 Grocery Store         Expense:Food:Groceries       $52.00      $132.00
end test
