end test

test reg p: --uncleared
2014-06-30 Pending              P:U                       10.00 EUR    10.00 EUR
                                P:P                       10.00 EUR    20.00 EUR
end test

test reg p: --pending
2014-06-30 Pending              P:U                       10.00 EUR    10.00 EUR
                                P:P                       10.00 EUR    20.00 EUR
end test

test reg p: --cleared
2014-06-30 Pending              P:C                       10.00 EUR    10.00 EUR
end test

test reg c: --uncleared
2014-06-30 Cleared              C:P                       10.00 EUR    10.00 EUR
end test

test reg c: --pending
2014-06-30 Cleared              C:P                       10.00 EUR    10.00 EUR
end test

test reg c: --cleared
